{
  "name": "django_portfolio",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "compile": "sass --watch gallery/static/scss:gallery/static/css",
    "collectStatic": "python manage.py collectstatic --no-input",
    "watch": "npm-watch"
  },
  "watch": {
    "compile": {
      "patterns": [
        "gallery/static/scss/**/*"
      ],
      "quiet": false
    },
    "collectStatic": {
      "patterns": [
        "gallery/static/**/**",
        "gallery/static/*"
      ],
      "quiet": false
    }
  },
  "author": "",
  "license": "MIT",
  "description": "",
  "dependencies": {
    "npm-watch": "^0.13.0",
    "sass": "^1.77.8"
  }
}
